<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
    <modules jcr:primaryType="jnt:modules">
        <user-password-authentication-api j:dependencies="" jcr:primaryType="jnt:module">
            <files jcr:primaryType="jnt:folder"/>
            <contents jcr:primaryType="jnt:contentFolder">
                <mfaEmailCode jcr:primaryType="upa:mfaEmailCode"/>
            </contents>
            <templates jcr:primaryType="jnt:templatesFolder">
                <files jcr:primaryType="jnt:folder"/>
                <contents jcr:primaryType="jnt:contentFolder"/>
                <base jcr:primaryType="jnt:template">
                    <emailCodeTemplate jcr:primaryType="jnt:contentTemplate"
                                         j:applyOn="upa:mfaEmailCode"
                                         j:view="emailCodeView"
                                         j:hiddenTemplate="true"
                                         j:priority="1"/>
                </base>
            </templates>
        </user-password-authentication-api>
    </modules>
</content>
